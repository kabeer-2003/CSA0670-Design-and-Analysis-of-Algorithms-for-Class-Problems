#include<stdio.h>
void swap(int x, int y)
{
	int temp=x;
	x=y;
	y=temp;
	printf("\nAfter swapping:\nx:%d\t  y:%d",x,y);
}
int main()
{
	int a,b;
	printf("Enter the first number:");
	scanf("%d",&a);
	printf("Enter the second number:");
	scanf("%d",&b);
	printf("Before Swapping:\nx:%d\t  y:%d ",a,b);
	swap(a,b);
	return 0;
}
